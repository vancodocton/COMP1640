{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "fontawesome-iconpicker@3.2.0",
      "destination": "wwwroot/lib/fontawesome-iconpicker/"
    },
    {
      "provider": "jsdelivr",
      "library": "@fortawesome/fontawesome-free@6.0.0",
      "destination": "wwwroot/lib/fortawesome/fontawesome-free/"
    },
    {
      "provider": "unpkg",
      "library": "@microsoft/signalr@latest",
      "destination": "wwwroot/js/signalr/"
    }
  ]
}